{
  "name": "facer",
  "version": "1.0.0",
  "description": "Facer - Realtime Face Recognition in the browser",
  "author": "Goran Jovanov",
  "private": true,
  "scripts": {
    "api": "cross-env NODE_ENV=development node api/start.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production pm2 start server/index.js -i 0 --attach -f",
    "start-simple": "cross-env NODE_ENV=production node ./server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/vuetify": "^1.11.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "face-api.js": "^0.21.0",
    "multer": "^1.4.2",
    "nuxt": "^2.15.3",
    "pm2": "^5.1.1",
    "rimraf": "^3.0.0",
    "sharp": "^0.29.0",
    "vuetify": "^2.5.3",
    "webrtc-adapter": "^7.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.4",
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@vue/test-utils": "^2.0.0-beta.29",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "browser-env": "^3.2.6",
    "eslint": "^7.0.0",

    "eslint-loader": "^4.0.2",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": ">=5.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "nodemon": "*",
    "standard": "^16.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.1.1"
  }
}
